adminLogin: CktlAdminLogin {
    loginResponder = loginResponder;
}

defaultPage: CktlDefaultPage {
	pageTitle = "ServeurPlanning";
}

imgClefs: WOImage {
    src = cktlApp.imageClefsSrc;
}

imgLigneJaune: WOImage {
  border = "0";
  width = "100%";
  height = "6";
  src = cktlApp.imageLigneSrc;
}

imgLogo: WOImage {
  border = "0";
  alt = "Logo Universite";
