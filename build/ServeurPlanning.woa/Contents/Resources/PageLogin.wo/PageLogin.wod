  mail = cktlApp.contactMail;
  mailLabel = "adressez-vous &agrave; l'administrateur en cliquant sur ce lien";
  //mailTip = "adressez-vous &agrave; l'administrateur en cliquant sur ce lien";
}

strCopyright: WOString {
  escapeHTML = false;
  value = spApp.appCktlVersion.htmlVersion;
}